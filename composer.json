{
    "name": "joeladam518/curtain-call-wp",
    "description": "CMS for theatres looking to create productions, cast, and crew custom posts.",
    "keywords": ["WordPress", "theatre", "theater", "productions", "plays", "cast", "crew"],
    "license": "MIT",
    "type": "project",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^2.25.2"
    },
    "require-dev": {
        "laravel/homestead": "^10.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "src"
    },
    "autoload": {
        "psr-4": {
            "CurtainCallWP\\": [ "plugin/" ]
        },
        "files": [
            "plugin/functions.php"
        ]
    }
}
